    "aria-query": "^5.3.0",
    "excalibur": "^0.29.3",
    "jsdom": "^24.0.0",
    "playwright": "^1.44.1",
    "redent": "^4.0.0",
    "typescript": "^5.2.2",
    "vite": "^5.0.8",
    "vite-plugin-solid": "^2.10.2",
    "vitest": "^1.6.0"
