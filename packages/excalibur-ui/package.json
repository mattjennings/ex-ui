{
  "name": "excalibur-ui",
  "version": "0.0.0-4",
  "type": "module",
  "author": "mattjennings",
  "description": "A UI library for Excalibur.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/mattjennings/excalibur-ui",
    "directory": "packages/excalibur-ui"
  },
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/mattjennings/excalibur-ui/issues"
  },
  "homepage": "https://github.com/mattjennings/excalibur-ui",
  "keywords": [
    "excalibur",
    "ui",
    "solid",
    "jsx"
  ],
  "files": [
    "dist"
  ],
  "main": "./dist/excalibur-ui.umd.cjs",
  "module": "./dist/excalibur-ui.js",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    },
    "./jsx-runtime": {
      "types": "./dist/jsx-runtime.d.ts",
      "import": "./dist/jsx-runtime.js",
      "require": "./dist/jsx-runtime.cjs"
    }
  },
  "scripts": {
    "dev": "vite build --watch",
    "build": "vite build && tsc",
    "prepublishOnly": "npm run build",
    "test": "vitest"
  },
  "devDependencies": {
    "@adobe/css-tools": "^4.3.3",
    "@types/node": "^20.12.12",
    "@vitest/browser": "2.0.0-beta.5",
    "excalibur": "^0.29.3",
    "expect-dom": "file:../expect-dom",
    "jsdom": "^24.0.0",
    "looks-same": "^9.0.0",
    "playwright": "^1.44.1",
    "typescript": "^5.2.2",
    "vite": "^5.0.8",
    "vite-plugin-dts": "^3.9.1",
    "vite-plugin-solid": "^2.10.2",
    "vitest": "2.0.0-beta.5",
    "solid-js": "^1.8.17"
  },
  "dependencies": {
    "csstype": "^3.1.3"
  },
  "peerDependencies": {
    "excalibur": ">=0.29",
    "solid-js": ">=1.8"
  }
}
